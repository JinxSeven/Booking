    <property name="styleSheet">
     <string notr="true">/* Base styles */
QTabWidget::pane {
  border: none;  /* Remove default border */
  background-color: #212121; /* Dark background */
  color: #fff;  /* Light text color */
  font: 8.5pt &quot;Poppins&quot;;
}

/* Tab bar positioning */
QTabWidget::tab-bar:top {
  top: 4px; /* Adjust top margin for better visual separation */
  font: 10pt &quot;Poppins&quot;;
}

QTabWidget::tab-bar:bottom {
  bottom: 4px;
  font: 10pt &quot;Poppins&quot;;
}

QTabWidget::tab-bar:left,
QTabWidget::tab-bar:right {
  left: 0;  /* Remove default left/right margin for centered tabs */
  right: 0;
  font: 10pt &quot;Poppins&quot;;
}

/* Tab styles */
QTabBar::tab {
  border: none;
  border-bottom: 1px solid #424242; /* Separator between tabs */
  background-color: #303030; /* Slightly lighter background for inactive tabs */
  color: #ccc; /* Lighter text color for inactive tabs */
  padding: 8px 4px; /* Adjust padding for better spacing */
  border-top-left-radius: 4px;
  border-top-right-radius: 4px;
  font: 8.5pt &quot;Poppins&quot;;
}

/* Selected tab styles */
QTabBar::tab:selected {
  background-color: #424242; /* Darker background for selected tab */
  color: #fff; /* White text color for selected tab */
  border-bottom-color: transparent; /* Remove bottom separator for selected tab */
}

/* Hover effect for inactive tabs */
QTabBar::tab:!selected:hover {
  background-color: #373737; /* Slightly darker background on hover */
}

/* Margins for inactive tabs */
QTabBar::tab:top:!selected {
  margin-top: 0; /* Remove top margin for cleaner look */
}

QTabBar::tab:bottom:!selected {
  margin-bottom: 0; /* Remove bottom margin for cleaner look */
}

/* Minimum size for tabs */
QTabBar::tab:top,
QTabBar::tab:bottom {
  min-width: 8em; /* Adjust minimum width for better readability */
}

QTabBar::tab:left,
QTabBar::tab:right {
  min-height: 8em; /* Adjust minimum height for better readability */
}

/* Last tab and single tab handling */
QTabBar::tab:top:last,
QTabBar::tab:bottom:last,
QTabBar::tab:top:only-one,
QTabBar::tab:bottom:only-one {
  border-top-left-radius: 4px;
  border-top-right-radius: 4px;
  border-bottom-right-radius: 4px;
  border-bottom-left-radius: 4px; /* Apply rounded corners for last or single tab */
}

QTabBar::tab:left:!selected,
QTabBar::tab:right:!selected {
  margin-left: 0; /* Remove left margin for cleaner look */
  margin-right: 4px; /* Add right margin for spacing */
}

QTabBar::tab:left:selected,
QTabBar::tab:right:selected {
  border-left-color: transparent; /* Remove left separator for selected tab */
  border-right-color: transparent; /* Remove right separator for selected tab */
}

QTabBar::tab:left:last,
QTabBar::tab:right:last,
QTabBar::tab:left:only-one,
QTabBar::tab:right:only-one {
  border-bottom-left-radius: 4px;
  border-bottom-right-radius: 4px; /* Apply rounded corners for last or single tab */
}
